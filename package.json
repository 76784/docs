{
  "name": "docs",
  "version": "1.0.0",
  "description": "The new-to-be handlebars documentation site",
  "private": true,
  "scripts": {
    "dev": "vuepress dev src ",
    "clean": "rm -rf target  node_modules/@vuepress/core/node_modules/.cache/",
    "build": "vuepress build src",
    "preview": "npm run build && cd target && static-server",
    "lintfix": "eslint --fix 'src/.vuepress/**/*.{js,vue}' ",
    "format": "prettier --write 'src/**/*.{js,css,json,md,vue}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handlebars-lang/docs.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/handlebars-lang/docs/issues"
  },
  "homepage": "https://github.com/handlebars-lang/docs#readme",
  "devDependencies": {
    "@vuepress/plugin-pwa": "^1.0.0-rc.1",
    "axios": "^0.18.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-vue": "^5.2.3",
    "fs-extra": "^8.1.0",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "markdown-it": "^8.4.2",
    "prettier": "^1.18.2",
    "semver": "^6.3.0",
    "static-server": "^2.2.1",
    "vuepress": "^1.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,vue}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "babel-eslint": "^10.0.2",
    "codemirror": "^5.48.2",
    "handlebars": "^4.1.2",
    "highlight.js": "^9.15.9",
    "json5": "^2.1.0",
    "vue-cm": "^1.1.0"
  }
}

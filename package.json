{
  "name": "docs",
  "version": "1.0.0",
  "description": "The new-to-be handlebars documentation site",
  "private": true,
  "scripts": {
    "dev": "vuepress dev src ",
    "clean": "rm -rf target  node_modules/@vuepress/core/node_modules/.cache/",
    "build": "vuepress build src",
    "preview": "npm run build && cd target && static-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handlebars-lang/docs.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/handlebars-lang/docs/issues"
  },
  "homepage": "https://github.com/handlebars-lang/docs#readme",
  "devDependencies": {
    "axios": "^0.18.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "markdown-it": "^8.4.2",
    "prettier": "^1.17.1",
    "semver": "^6.1.1",
    "static-server": "^2.2.1",
    "vuepress": "^1.0.0-beta.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "codemirror": "^5.47.0",
    "handlebars": "^4.1.2",
    "highlight.js": "^9.15.8",
    "json5": "^2.1.0",
    "vue-cm": "^1.1.0"
  }
}
